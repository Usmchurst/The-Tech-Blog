<section class="dashboard-section">
    {{#each posts}}
    <div class="border_darkblue mt-4">
      <div
        class="bg_darkblue text-white p-2 d-flex justify-content-between"
        onclick="window.location.pathname= `/posts/edit/{{id}}`"
        style="min-height: 100px ;"
      >
        <h4>{{title}}</h4>
        <p>{{dateFormat createdAt}}</p>
      </div>
    </div>
  {{/each}}
    <a href="/post/add" class="btn text-white btn-lg newpost_btn">New Post</a>

</section>